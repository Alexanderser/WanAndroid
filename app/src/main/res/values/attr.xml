<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="returnImgSrc" format="reference"/>
        <attr name="menuImgSrc" format="reference"/>
        <attr name="titleText" format="string"/>
        <attr name="subtitleText" format="string"/>
        <attr name="titleColor" format="color"/>
        <attr name="subtitleColor" format="color"/>
        <attr name="returnImgTintColor" format="color"/>
        <attr name="menuImgTintColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="PercentImageView">
        <attr name="piv_basics" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
        <attr name="piv_percent" format="float" />
    </declare-styleable>

</resources>